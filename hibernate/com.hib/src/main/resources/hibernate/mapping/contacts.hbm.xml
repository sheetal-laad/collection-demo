<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class table="contacts" lazy="false" name="HibernateDemo.com.hib.Contacts">
<id column="id" type="int" name="id">
<generator class="increment"></generator></id>
<property name="fname" type="string" column="fname"/>
<property name="lname" type="string" column="lname"/>




</class>






</hibernate-mapping>